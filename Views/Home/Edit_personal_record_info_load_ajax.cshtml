@model Im.Models.ApplicationUser
   @{ string class_li_div = "Edit_personal_record_info_load_ajax_link_st";}

<div class="Edit_personal_record_info_load_ajax_osn_left_block">
    


</div>

<div class="Edit_personal_record_info_load_ajax_osn_right_block">
   
        <ul>

            @foreach (string i in ViewBag.list_menu)
            {
                <li>
                    @if ((string)ViewBag.click == i)
                    {
                        class_li_div = "Edit_personal_record_info_load_ajax_link_not_st";
                    }
                    @if ((string)ViewBag.click != i)
                    {
                        class_li_div = "Edit_personal_record_info_load_ajax_link_st";
                    }
                    <div class=@class_li_div>



                        @Ajax.ActionLink(i, "Edit_personal_record_info_load_ajax", "Home", new { obg = i },
new AjaxOptions { UpdateTargetId = "Edit_personal_record_info_load_ajax_main_div_id" }, new { @class = "link_standart_for_div" })
                    </div>
                </li>
            }



        </ul>
    
    

    </div>

